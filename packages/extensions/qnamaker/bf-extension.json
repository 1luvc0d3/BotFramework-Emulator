{
  "name": "QnA Maker",
  "client": {
    "inspectors": [
      {
        "name": "QnA",
        "src": "index.html",
        "criteria": [
          {
            "path": "$.type",
            "value": "event"
          },
          {
            "path": "$.event",
            "value": "QnATraceInfo"
          }
        ],
        "summaryText": [
          "$.label"
        ],
        "accessories": [
          {
            "id": "train",
            "states": {
              "default": {
                "label": "Train",
                "icon": "Refresh"
              },
              "working": {
                "label": "Training",
                "icon": "Spinner"
              }
            }
          },
          {
            "id": "publish",
            "states": {
              "default": {
                "label": "Publish",
                "icon": "Share"
              }
            }
          }
        ]
      }
    ]
  }
}