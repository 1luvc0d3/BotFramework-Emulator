{
  "name": "@bfemulator/client",
  "version": "1.0.0",
  "private": true,
  "main": "./build/index.html",
  "homepage": "./",
  "scripts": {
    "build": "npm run copy:external && react-app-rewired build --scripts-version react-scripts-ts",
    "clean": "rimraf build",
    "copy:external": "gulp copy",
    "eject": "react-app-rewired eject --scripts-version react-scripts-ts",
    "prepare": "npm run clean && npm run build",
    "start": "npm run copy:external && cross-env BROWSER=none react-app-rewired start --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --env=jsdom --scripts-version react-scripts-ts"
  },
  "devDependencies": {
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/request": "^2.47.0",
    "babel-loader": "^7.1.3",
    "babel-preset-react-app": "^3.1.1",
    "cross-env": "^5.1.3",
    "gulp": "^4.0.0",
    "react-app-rewired": "^1.4.1",
    "react-dev-utils": "^5.0.0",
    "react-scripts-ts": "^2.13.0",
    "rimraf": "^2.6.2",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "@bfemulator/app-shared": "^1.0.0",
    "@bfemulator/custom-botframework-webchat": "^0.11.5-c0",
    "@bfemulator/sdk-client": "^1.0.0",
    "@bfemulator/sdk-shared": "^1.0.0",
    "classnames": "^2.2.5",
    "electron-ipcrenderer-websocket": "^0.1.0-0",
    "glamor": "^2.20.40",
    "immutable": "^3.8.2",
    "lodash": "^4.17.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-split-pane": "^0.1.77",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "redux-websocket-bridge": "^0.3.0-0",
    "request": "^2.83.0",
    "rxjs": "^5.5.6",
    "sockjs-client": "^1.1.4"
  }
}
