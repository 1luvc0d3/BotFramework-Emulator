{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "preserveConstEnums": true,
        "target": "es5",
        "jsx": "react",
        "sourceMap": true,
        "noImplicitAny": false,
        "noImplicitThis": true,
        "noEmitOnError": true,
        "outDir": "dist"
    },
    "exclude": [
        "node_modules",
        "dist",
        ".vscode"
    ]
}